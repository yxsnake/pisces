    <url>https://github.com/yxsnake/pisces</url>

    <licenses>
        <license>
            <name>The Apache Software License, Version 2.0</name>
            <url>https://github.com/yxsnake/pisces/blob/main/LICENSE</url>
            <distribution>repo</distribution>
        </license>
    </licenses>
    <scm>
        <url>https://github.com/yxsnake/pisces</url>
        <connection>https://github.com/yxsnake/pisces.git</connection>
        <developerConnection>scm:git:ssh://git@github.com:yxsnake/pisces.git</developerConnection>
        <tag>HEAD</tag>
