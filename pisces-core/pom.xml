    <issueManagement>
        <system>GitHub Issues</system>
        <url>https://github.com/yxsnake/pisces/issues</url>
    </issueManagement>
    <developers>
        <developer>
            <id>snake</id>
            <name>snake</name>
            <email>370696614@qq.com</email>
            <organization>https://github.com/yxsnake/pisces</organization>
            <organizationUrl>https://github.com/yxsnake/pisces</organizationUrl>
            <timezone>+8</timezone>
            <roles>
                <role>Project Manager</role>
                <role>Architect</role>
            </roles>
            <properties>
                <picUrl>https://i1.hdslb.com/bfs/face/98a570a6c6d6a263bcb0cba9e15e492125e9d310.jpg@120w_120h_1c</picUrl>
            </properties>
        </developer>
    </developers>

    <properties>
